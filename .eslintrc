{
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "require-jsdoc": "off",
        "no-console": "off",
        "no-param-reassign": "off",
        "no-unused-vars": "warn",
        "indent": ["error", 4],
        "linebreak-style": "off"
    }
}